{
  "name": "@make-software/cspr-ui",
  "version": "1.0.0",
  "main": "./dist/lib/index.js",
  "repository": "git://github.com/make-software/cspr-ui-library",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/make-software"
  },
  "scripts": {
    "build:dist": "shx rm -rf ./dist && npm run build:esm && npm run build:cjs && npm run copy-assets",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "copy-assets": "shx cp -r src/lib/assets/ dist/lib/assets/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "start": "react-scripts start -p 6006",
    "test": "react-scripts test",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc"
  },
  "dependencies": {
    "@formatjs/intl": "2.7.2",
    "big.js": "^6.2.1",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.30.0",
    "downshift": "^7.6.0",
    "facepaint": "^1.2.1",
    "i18next": "^23.4.5",
    "react-identicons": "^1.2.5",
    "react-inlinesvg": "^3.0.2",
    "react-loading-skeleton": "^3.3.1",
    "react-modal": "^3.16.1",
    "react-scripts": "5.0.1",
    "reakit": "^1.3.11",
    "styled-components": "^5.2.1",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@react-theming/storybook-addon": "^1.1.10",
    "@storybook/addon-actions": "^7.0.6",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-mdx-gfm": "^7.0.6",
    "@storybook/addon-styling": "^1.0.1",
    "@storybook/manager-api": "^7.0.7",
    "@storybook/node-logger": "^7.0.6",
    "@storybook/preset-create-react-app": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.7",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/facepaint": "^1.2.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.18",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0",
    "shx": "^0.3.4",
    "storybook": "^7.0.6",
    "typescript": "^4.3.5",
    "webpack": "^5.76.2"
  }
}
